{
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "jquery": true,
    "jest": true
  },
  "globals": {
    "jquery": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [],
  "rules": {
    "strict": [
      2,
      "global"
    ],
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline"
      }
    ],
    "arrow-spacing": [
      "error",
      {
        "before": true,
        "after": true
      }
    ],
    "indent": [
      "error",
      2
    ],
    "max-len": 0,
    "padded-blocks": [
      "error",
      {
        "blocks": "always"
      }
    ],
    "camelcase": 2,
    "curly": 0,
    "eqeqeq": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-bitwise": 2,
    "no-useless-escape": 1,
    "no-console": 0,
    "no-use-before-define": 2,
    "no-case-declarations": 0,
    "new-cap": 2,
    "no-void": 0,
    "no-caller": 2,
    "no-undef": 2,
    "no-unused-vars": 0,
    "no-trailing-spaces": 0,
    "quotes": [
      2,
      "single"
    ],
    "spaced-comment": [
      "error",
      "always",
      {
        "exceptions": [
          "-",
          "+"
        ]
      }
    ],
    "react/prop-types": 0,
    "react/no-find-dom-node": 0
  }
}
